{
  "name": "math-game",
  "version": "0.0.1",
  "description": "A math puzzle game",
  "author": "Ken Fehling",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --port 1234 --progress --colors --history-api-fallback --config webpack.config.development.babel.js",
    "test": "mocha --compilers ts:ts-node/register --recursive test/unit",
    "clean": "rm -rf build",
    "build": "npm run clean; webpack --config webpack.config.production.babel.js; cp index.html build; cp -r static/* build/static",
    "publish": "git subtree push --prefix build origin gh-pages"
  },
  "keywords": [
    "math",
    "puzzle",
    "game"
  ],
  "dependencies": {
    "bowser": "^1.6.1",
    "isomorphic-fetch": "^2.2.1",
    "ramda": "^0.23.0",
    "react": "^15.5.3",
    "react-burger-menu": "^2.0.0",
    "react-dnd": "^2.3.0",
    "react-dnd-html5-backend": "^2.3.0",
    "react-dnd-touch-backend": "^0.3.6",
    "react-redux": "^5.0.3",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-treeview": "^0.4.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0"
  },
  "devDependencies": {
    "@types/history": "^4.5.0",
    "@types/ramda": "0.0.2",
    "@types/react": "^15.0.11",
    "@types/react-dnd": "^2.0.32",
    "@types/react-dom": "^0.14.20",
    "@types/react-redux": "^4.4.36",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-1": "^6.13.0",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "file-loader": "^0.11.1",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "ts-loader": "^2.0.3",
    "ts-node": "^2.1.0",
    "typescript": "^2.2.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.4.2"
  }
